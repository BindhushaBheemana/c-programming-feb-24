/*Write a program to print the following pattern

1   3   5    7
5   7   9  11
9  11 13 15
*/
#include <stdio.h>

int main() {
    int row, col, num;

    num = 1;
    for (row = 1; row <= 3; row++) {
        for (col = 0; col < 4; col++) {
            printf("%d ", num);
            num += 2;
        }
        printf("\n");
        num -= 6;
    }

    return 0;
}
